<!DOCTYPE html>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:keke='urn:import:com.risetek.keke.client.ui'>
	<g:DockLayoutPanel unit='PX' styleName="risefront"
		width="100%" height="100%">
		<g:north size="10">
			<g:HTMLPanel></g:HTMLPanel>
		</g:north>
		<g:center>
			<g:HTMLPanel width="100%" height="100%">
				<TABLE align="center" width="1200px" height="500px" border="1"
					class="workview">
					<TR>
						<TD width="300px" height="100%">
							<g:HTMLPanel width="100%" height="100%">
								<TABLE width="100%" height="100%">
									<TR>
										<TD>
											<keke:LoggerComposite ui:field="logger"></keke:LoggerComposite>
										</TD>
									</TR>
									<TR>
										<TD>
											<keke:ControlPanel width="100%" height="100%"></keke:ControlPanel>
										</TD>
									</TR>
								</TABLE>
							</g:HTMLPanel>
						</TD>
						<TD width="400px" height="100%">
							<g:HTMLPanel width="100%" height="100%" styleName="caller">
								<TABLE width="100%" height="100%">
									<TR height="45%" width="100%">
										<TD>
											<keke:CallerStackComposite
												ui:field="caller"></keke:CallerStackComposite>
										</TD>
									</TR>
									<TR width="100%">
										<TD>
											<keke:HistoryStackComposite
												ui:field="history"></keke:HistoryStackComposite>
										</TD>
									</TR>
								</TABLE>
							</g:HTMLPanel>
						</TD>
						<TD>
							<g:HTMLPanel ui:field="skins" styleName="d3view"
								width="100%" height="100%">
								<TABLE width="100%" height="100%" border="0">
									<TR>
										<TD>D3M</TD>
										<TD width="300px">
											<TABLE width="100%" height="100%" border="0">
												<TR>
													<TD>D3View</TD>
												</TR>
												<TR>
													<TD>
														<keke:KekesComposite ui:field="kekeComposite"></keke:KekesComposite>
													</TD>
												</TR>
												<TR>
													<TD>熊可牵</TD>
												</TR>
											</TABLE>

										</TD>
									</TR>
								</TABLE>
							</g:HTMLPanel>
						</TD>
					</TR>
				</TABLE>
			</g:HTMLPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>

