<!DOCTYPE html>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:keke='urn:import:com.risetek.keke.client.ui'>
	<ui:style type="com.risetek.keke.client.Risetek_keke.ViewStyle">
		.risefront {
			background: #aa8ebf;
			text-align: center;
			cellspacing: 0px;
			cellpading: 0px;
		}
		
		.risefront td {
			border: solid white;
			border-width: 0px;
		}
		
		.machine {
			background: #2a8ebf;
			text-align: center;
			cellspacing: 0px;
			cellpading: 0px;
		}
		
		.machine td {
			border: solid white;
			border-width: 1px;
		}
		
		.keke-table {
			border: 1px solid #999;
			border-collapse: collapse;
			text-align: center;
			cellspacing: 0px;
			cellpading: 0px;
		}
		
		.keke-table td {
			width: 32em;
			height: 6em;
			border: solid #999;
			border-width: 0px;
			padding-top: 0px;
			padding-left: 0px;
			padding-bottom: 0px;
			padding-right: 0px;
		}
		
		.hilight {
			background: yellow;
		}

		.button {
			width: 6em;
		}

	</ui:style>

	<g:DockLayoutPanel unit='EM' styleName="{style.risefront}">
		<g:north size="4">
			<g:HTML></g:HTML>
		</g:north>
		<g:center>
			<g:HTMLPanel width="100%">
				<TABLE align="center">
					<TR>
						<TD width="60%">
							<g:HTMLPanel ui:field="skins" styleName="{style.machine}">
								<TABLE>
									<TR>
										<TD colspan="2">Risetek</TD>
									</TR>
									<TR>
										<TD>Risetek</TD>
										<TD>
											<TABLE align="center" border="0">
												<TR>
													<TD>
													<g:DecoratorPanel>
															<g:HTMLPanel width="100%">
																<TABLE align="center">
																	<TR>
																		<TD>
																			<g:Grid ui:field="keke" width="100%"
																				styleName="{style.keke-table}"></g:Grid>
																		</TD>
																	</TR>
																</TABLE>
															</g:HTMLPanel>
													</g:DecoratorPanel>
													</TD>
												</TR>
											</TABLE>
										</TD>
									</TR>
									<TR>
										<TD colspan="2">Risetek</TD>
									</TR>
								</TABLE>
							</g:HTMLPanel>
						</TD>
						<TD>
							<TABLE width="100%">
								<TR>
									<TD>
										<span ui:field="logger">系统信息</span>
									</TD>
								</TR>
								<TR>
									<TD>
										<g:Button ui:field="card" styleName="gwt-Button {style.button}">刷卡</g:Button>
									</TD>
								</TR>
								<TR>
									<TD>
										<TABLE width="100%">
											<TR>
												<TD colspan="2">
													<g:Button ui:field="up" styleName="gwt-Button {style.button}">上</g:Button>
												</TD>
											</TR>
											<TR>
												<TD>
													<g:Button ui:field="left" styleName="gwt-Button {style.button}">左</g:Button>
												</TD>
												<TD>
													<g:Button ui:field="right" styleName="gwt-Button {style.button}">右</g:Button>
												</TD>
											</TR>
											<TR>
												<TD colspan="2">
													<g:Button ui:field="down" styleName="gwt-Button {style.button}">下</g:Button>
												</TD>
											</TR>
										</TABLE>
									</TD>
								</TR>
							</TABLE>
						</TD>
						<TD><keke:HIDComposite></keke:HIDComposite></TD>
					</TR>
				</TABLE>
			</g:HTMLPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>

