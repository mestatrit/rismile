<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:HTMLPanel>
		<table border="0" cellpadding="0" cellspacing="5px">
			<tr>
				<td width="100%" height="20px" align="center" colspan="2">
					<g:Label ui:field="label"></g:Label>
				</td>
			</tr>
			<tr>
				<td height="40px" align="center" colspan="2">
					<g:VerticalPanel ui:field="mainPanel"></g:VerticalPanel>
				</td>
			</tr>
			<tr>
				<td align="center" colspan="2">
					<g:Label ui:field="message">&nbsp;</g:Label>
				</td>
			</tr>
			<tr align="center">
				<td width="50%">
					<g:Button ui:field="submit"></g:Button>
				</td>
				<td>
					<g:Button ui:field="cancel">取消</g:Button>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 