<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.topLeft {
			background: url(images/flag.png) no-repeat 0px 0px;
			-background: url(images/flag_ie6.png) no-repeat 0px 0px;
			background-color: #dce7f7;
		}
		
		.topRight {
			background: url(images/flag.png) no-repeat -7px 0px;
			-background: url(images/flag_ie6.png) no-repeat -7px 0px;
			background-color: #dce7f7;
		}
		
		.topLeftInner,.topRightInner {
			width: 7px;
			height: 7px;
		}
		
		.topMiddleInner {
			height: 1px;
		}
		
		.topMiddle {
			vertical-align: top;
			background-color: #dce7f7;
			border-top-width: 1px;
			border-top-style: solid;
			border-top-color: #2A8EBF;
		}
		
		.middleLeft {
			border-left-width: 1px;
			border-left-style: solid;
			border-left-color: #2A8EBF;
			border-right-width: 1px;
			border-right-style: solid;
			border-right-color: #2A8EBF;
		}
		
		.bottomLeft {
			background: url(images/flag.png) no-repeat 0px -7px;
			-background: url(images/flag_ie6.png) no-repeat 0px -7px;
			background-color: #dce7f7;
		}
		
		.bottomRight {
			background: url(images/flag.png) no-repeat -7px -7px;
			-background: url(images/flag_ie6.png) no-repeat -7px -7px;
			background-color: #dce7f7;
			-background-color: #dce7f7;
		}
		
		.bottomLeftInner,.bottomRightInner {
			width: 7px;
			height: 7px;
		}
		
		.bottomMiddleInner {
			height: 1px;
		}
		
		.bottomMiddle {
			vertical-align: bottom;
			background-color: #dce7f7;
			border-bottom-width: 1px;
			border-bottom-style: solid;
			border-bottom-color: #2A8EBF;
		}
	</ui:style>
	<g:HTMLPanel>
		<table border="0" cellspacing="0" cellpadding="0">
			<tr>
				<td>
					<table width="100%" border="0" cellspacing="0" cellpadding="0">
						<tr>
							<td class="{style.topLeft}">
								<table width="7" height="7" border="0" cellspacing="0" cellpadding="0">
									<tr><td></td></tr>
								</table>
							</td>
							<td class="{style.topMiddle}" width="99%">
								<table width="100%" border="0" cellspacing="0" cellpadding="0">
									<tr><td></td></tr>
								</table>
							</td>
							<td class="{style.topRight}">
								<table width="7" height="7" border="0" cellspacing="0" cellpadding="0">
									<tr><td></td></tr>
								</table>
							</td>
						</tr>
					</table>
				</td>
			</tr>
			<tr>
				<td>
					<table width="100%" border="0" cellspacing="0" cellpadding="0">
						<tr>
							<td class="{style.middleLeft}">
								<g:FlowPanel ui:field="body"></g:FlowPanel>
							</td>
						</tr>
					</table>

				</td>
			</tr>
			<tr>
				<td>
					<table width="100%" border="0" cellpadding="0" cellspacing="0">
						<tr>
							<td class="{style.bottomLeft}">
								<table width="7" height="7" border="0" cellspacing="0" cellpadding="0">
									<tr><td></td></tr>
								</table>
							</td>
							<td class="{style.bottomMiddle}" width="99%">
								<table width="7" height="7" border="0" cellspacing="0" cellpadding="0">
									<tr><td></td></tr>
								</table>
							</td>
							<td class="{style.bottomRight}">
								<table width="7" height="7" border="0" cellspacing="0" cellpadding="0">
									<tr><td></td></tr>
								</table>
							</td>
						</tr>
					</table>
				</td>
			</tr>
			<tr>
				<td height="5"><div></div></td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 